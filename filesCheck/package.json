{
  "name": "filesCheck",
  "version": "1.0.0",
  "description": "Script that checks consistency of files expected by sBots",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benkio/sBots.git"
  },
  "keywords": [
    "id3v2",
    "tags",
    "mp3",
    "mp4",
    "mediainfo"
  ],
  "author": "Enrico Benini (Benkio)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benkio/sBots/issues"
  },
  "homepage": "https://github.com/benkio/sBots#readme",
  "dependencies": {
    "gts": "^6.0.2",
    "mediainfo-parser": "^1.1.5",
    "node-id3": "^0.2.9",
    "tsx": "^4.20.5",
    "winston": "^3.18.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "gts": "^6.0.2",
    "prettier": "3.6.2",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "lint": "gts lint",
    "check": "prettier . --check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "format": "prettier . --write",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "start": "tsx ./src/index.ts",
    "watch": "tsc -w"
  }
